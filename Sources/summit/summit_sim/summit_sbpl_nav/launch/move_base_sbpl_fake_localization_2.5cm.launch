<?xml version="1.0"?>
<launch>
  <master auto="start"/>
  <param name="/use_sim_time" value="true"/>

  <!-- launch robot state publisher (just to see the robot chassis) in rviz -->
  <include file="$(find summit_description)/launch/summit_state.launch"/>
  
  <!-- launch move_base_node for planning and control -->
  <!-- include file="$(find sbpl_lattice_planner)/launch/move_base/move_base_sbpl.xml"/ -->
  <!-- remap from cmd_vel to cmd_vel1 -->
  <include file="$(find summit_sbpl_nav)/launch/move_base/move_base_sbpl.xml"/>
  
  <!-- adapts cmd_vel from diff to ackerman -->
  <node name="summit_sbpl_nav" pkg="summit_sbpl_nav" type="summit_sbpl_nav" respawn="false"/>

  <!-- node name="map_server" pkg="map_server" type="map_server" args="$(find summit_sbpl_nav)/worlds/robotnik.png 0.025"/ -->

  <!-- node pkg="stage" type="stageros" name="stageros" args="$(find summit_sbpl_nav)/worlds/robotnik.world" respawn="false"/ -->
    <!-- param name="base_watchdog_timeout" value="0.2"/ -->

  <node name="fake_localization" pkg="fake_localization" type="fake_localization" respawn="false"/> 

  <!-- nav_view replaced by rviz visualization and goal setting -->
  <!-- include file="$(find summit_sbpl_nav)/launch/rviz.xml"/ -->
</launch>
